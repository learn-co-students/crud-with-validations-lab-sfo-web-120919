<%= form_for(@song) do |f| %>
 
 <%= f.label :title, "Title" %>
 <%= f.text_field :title, :value => @song.title %>
 <%= f.label :artist_name, "Artist Name" %>
 <%= f.text_field :artist_name, :value => @song.artist_name %>
 <%= f.label :genre, "Genre" %>
 <%= f.text_field :genre, :value => @song.genre %>
 <%= f.label :release_year, "Release Year" %>
 <%= f.number_field :release_year, :value => @song.release_year %>
 <%= f.label :released, :value => "Released" %>
 <%= f.check_box :released, :value => @song.released %>
 <%= f.submit "Submit" %>  

<% end %>